module Anthropic
  module Models
    type error_response =
      { error: Anthropic::Models::error_object, type: :error }

    class ErrorResponse < Anthropic::BaseModel
      attr_accessor error: Anthropic::Models::error_object

      attr_accessor type: :error

      def initialize: (
        error: Anthropic::Models::error_object,
        ?type: :error
      ) -> void

      def to_hash: -> Anthropic::Models::error_response
    end
  end
end

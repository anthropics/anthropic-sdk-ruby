module Anthropic
  module Models
    type usage =
      {
        cache_creation: Anthropic::CacheCreation?,
        cache_creation_input_tokens: Integer?,
        cache_read_input_tokens: Integer?,
        inference_geo: String?,
        input_tokens: Integer,
        output_tokens: Integer,
        server_tool_use: Anthropic::ServerToolUsage?,
        service_tier: Anthropic::Models::Usage::service_tier?,
        speed: Anthropic::Models::Usage::speed?
      }

    class Usage < Anthropic::Internal::Type::BaseModel
      attr_accessor cache_creation: Anthropic::CacheCreation?

      attr_accessor cache_creation_input_tokens: Integer?

      attr_accessor cache_read_input_tokens: Integer?

      attr_accessor inference_geo: String?

      attr_accessor input_tokens: Integer

      attr_accessor output_tokens: Integer

      attr_accessor server_tool_use: Anthropic::ServerToolUsage?

      attr_accessor service_tier: Anthropic::Models::Usage::service_tier?

      attr_accessor speed: Anthropic::Models::Usage::speed?

      def initialize: (
        cache_creation: Anthropic::CacheCreation?,
        cache_creation_input_tokens: Integer?,
        cache_read_input_tokens: Integer?,
        inference_geo: String?,
        input_tokens: Integer,
        output_tokens: Integer,
        server_tool_use: Anthropic::ServerToolUsage?,
        service_tier: Anthropic::Models::Usage::service_tier?,
        speed: Anthropic::Models::Usage::speed?
      ) -> void

      def to_hash: -> {
        cache_creation: Anthropic::CacheCreation?,
        cache_creation_input_tokens: Integer?,
        cache_read_input_tokens: Integer?,
        inference_geo: String?,
        input_tokens: Integer,
        output_tokens: Integer,
        server_tool_use: Anthropic::ServerToolUsage?,
        service_tier: Anthropic::Models::Usage::service_tier?,
        speed: Anthropic::Models::Usage::speed?
      }

      type service_tier = :standard | :priority | :batch

      module ServiceTier
        extend Anthropic::Internal::Type::Enum

        STANDARD: :standard
        PRIORITY: :priority
        BATCH: :batch

        def self?.values: -> ::Array[Anthropic::Models::Usage::service_tier]
      end

      type speed = :standard | :fast

      module Speed
        extend Anthropic::Internal::Type::Enum

        STANDARD: :standard
        FAST: :fast

        def self?.values: -> ::Array[Anthropic::Models::Usage::speed]
      end
    end
  end
end

module Anthropic
  module Models
    type stop_reason = :end_turn | :max_tokens | :stop_sequence | :tool_use

    module StopReason
      extend Anthropic::Enum

      END_TURN: :end_turn
      MAX_TOKENS: :max_tokens
      STOP_SEQUENCE: :stop_sequence
      TOOL_USE: :tool_use

      def self?.values: -> ::Array[Anthropic::Models::stop_reason]
    end
  end
end
